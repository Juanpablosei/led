{
  "expo": {
    "name": "LEDAT",
    "slug": "ledgestion",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "ledat",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "es.arescoop.ledat",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ],
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true,
          "NSExceptionDomains": {
            "desarrollo.arescoop.es": {
              "NSExceptionAllowsInsecureHTTPLoads": true,
              "NSExceptionMinimumTLSVersion": "1.0",
              "NSExceptionRequiresForwardSecrecy": false
            }
          }
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "es.arescoop.ledat",
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "USE_FINGERPRINT"
      ],
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "fonts": [
      "./assets/fonts/Aeonik-Regular.otf",
      "./assets/fonts/Aeonik-Bold.otf",
      "./assets/fonts/Aeonik-Light.otf",
      "./assets/fonts/Aeonik-RegularItalic.otf",
      "./assets/fonts/Aeonik-BoldItalic.otf",
      "./assets/fonts/Aeonik-LightItalic.otf"
    ],
    "plugins": [
      "expo-router",
      "expo-navigation-bar",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-asset",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#E95460",
          "sounds": [],
          "mode": "production"
        }
      ],
      [
        "expo-web-browser",
        {
          "scheme": "ledat"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "da21058d-d19b-4eab-85da-39bb32e96140"
      }
    },
    "owner": "pablosei",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/da21058d-d19b-4eab-85da-39bb32e96140"
    }
  }
}
